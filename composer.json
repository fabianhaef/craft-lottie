{
  "name": "10vu10/craft-craft-lottie",
  "description": "Lottie Animator for Craft cms",
  "type": "craft-plugin",
  "license": "proprietary",
  "support": {
    "email": "admin@10vu10.ch"
  },
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.8.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main",
    "phpstan/phpstan": "^1.10.0"
  },
  "autoload": {
    "psr-4": {
      "vu\\craftlottie\\": "src/"
    }
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan analyse --memory-limit=256M",
    "rector": "rector process --ansi",
    "check": [
      "@check-cs",
      "@phpstan"
    ]
  },
  "extra": {
    "handle": "craft-lottie",
    "name": "craft-lottie",
    "developer": "10vu10",
    "documentationUrl": ""
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}